{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"moduleResolution": "NodeNext",
	},
	"include": ["./scripts/**/*.ts"]
}
