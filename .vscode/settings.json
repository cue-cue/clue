{
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"folderTemplates.fileTemplates": {
		"PublicApiFile": ["export {", "\tMain as <FTName>,", "} from './ui'"],
		"PublicApiUIFile": ["import Main from './Main/index.svelte'", "\t", "export {", "\tMain,", "}"],
		"TSConfig": ["{", "\t\"extends\": \"../../../../tsconfig.json\"", "}"],
		"MainSvelteFile": ["<script lang='ts'>", "\tinterface $$Props {", "\t\tclass?:string", "\t}", "\t", "\tlet className = ''", "\texport { className as class }", "\t", "</script>", "", "<div class={`<FTName> ${className}`}>", "</div>", "", "<style lang='sass'>", "\t.<FTName>", "</style>"],
		"PackageInfo": ["{", "\t\"name\": \"@cluue/<FTName | lowercase>\",", "\t\"version\": \"1.0.0\",", "\t\"private\": false,", "\t\"license\": \"MIT\",", "\t\"main\": \"src/index.js\",", "\t\"svelte\": \"src/index.js\",", "\t\"types\": \"src/index.d.ts\",", "\t\"type\": \"module\",", "\t\"author\": \"Nikita Fedorov\"", "}"],
		"PackageReadme": "# <FTName>",
		"PackagePublicApi": "// export * from './<FTName | pascalcase>/index.js'",
		"PackageModule": ["<script lang='ts'>", "\timport {generateClassNames} from '@cluue/utils'", "", "\tinterface $$Props {", "\t\tclass?:string", "\t}", "\t", "\tlet className = ''", "\texport { className as class }", "\t", "</script>", "", "<div class={generateClassNames(['<FTName | pascalcase>', className])}>", "</div>", "", "<style lang='sass'>", "\t.Clue<FTName | pascalcase>", "</style>"],
		"PackageModulePublicApi": ["import <FTName | pascalcase> from './<FTName | pascalcase>.svelte'", "", "export {", "\t<FTName | pascalcase>", "}"]
	},
	"folderTemplates.structures": [
		{
			"name": "Package Module",
			"omitParentDirectory": false,
			"openFilesWhenDone": ["./<FTName | pascalcase>.svelte"],
			"structure": [
				{
					"fileName": "./<FTName | pascalcase>.svelte",
					"template": "PackageModule"
				},
				{
					"fileName": "index.ts",
					"template": "PackageModulePublicApi"
				}
			]
		},
		{
			"name": "Package Module Component",
			"omitParentDirectory": true,
			"openFilesWhenDone": ["./<FTName | pascalcase>.svelte"],
			"structure": [
				{
					"fileName": "./<FTName | pascalcase>.svelte",
					"template": "PackageModule"
				}
			]
		},
		{
			"name": "Package TSConfig",
			"omitParentDirectory": true,
			"omitFTName": true,
			"openFilesWhenDone": ["./tsconfig.json"],
			"structure": [
				{
					"fileName": "./tsconfig.json",
					"template": "TSConfig"
				}
			]
		},
		{
			"name": "Package",
			"omitParentDirectory": false,
			"openFilesWhenDone": ["package.json"],
			"structure": [
				{
					"fileName": "package.json",
					"template": "PackageInfo"
				},
				{
					"fileName": "README.md",
					"template": "PackageReadme"
				},
				// {
				//   "fileName": "tsconfig.json",
				//   "template": "TSConfig"
				// },
				{
					"fileName": "src/index.ts",
					"template": "PackagePublicApi"
				}
			]
		}
	],
	"cSpell.words": ["cluue", "outclick"]
}
